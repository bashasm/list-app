<h1>List of Empoyee</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">First</th>
      <th scope="col">Last</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.firstName}}</td>
      <td>{{this.lastName}}</td>
    </tr>
    {{/each}}

  </tbody>
</table>